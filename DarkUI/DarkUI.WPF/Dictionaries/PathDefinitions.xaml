<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<PathGeometry x:Key="WideArrowUp" Figures="M 5 0 L 10 5 L 0 5 Z" />
	<PathGeometry x:Key="WideArrowDown" Figures="M 5 5 L 10 0 L 0 0 Z" />
	<PathGeometry x:Key="WideArrowLeft" Figures="M -5 5 L 0 10 L 0 0 Z" />
	<PathGeometry x:Key="WideArrowRight" Figures="M 5 5 L 0 10 L 0 0 Z" />
	<PathGeometry x:Key="NarrowArrowUp" Figures="M 3 -4 L 6 0 L 0 0 Z" />
	<PathGeometry x:Key="NarrowArrowDown" Figures="M 3 4 L 6 0 L 0 0 Z" />
	<PathGeometry x:Key="NarrowArrowRight" Figures="M 0 0 L 0 7 L 4 3.5 Z" />
	<PathGeometry x:Key="CheckMark" Figures="M 0 4 L 3 7 L 8 0" />
	<PathGeometry x:Key="TreeArrow" Figures="M 1 -1 1 7 5 3 Z" />
	<PathGeometry x:Key="TreeArrowChecked" Figures="M 5 0 0 5 5 5 Z" />
	<PathGeometry x:Key="GripperPattern" Figures="M 0 0 0 1 1 1 1 0 Z M 2 2 2 3 3 3 3 2 Z" />
</ResourceDictionary>
